ackermann_peter(m i32, n i32) :: i32 {
    if (m == 0) {
        return (n + 1);
    }
    if (n == 0) {
        return ackermann_peter((m - 1), 1);
    }
    return ackermann_peter((m - 1), ackermann_peter(m, (n - 1)));
}

main() {
    n = 0;
    loop {
        @print_i32(ackermann_peter(3, n));
        n = (n + 1);
        if (n == 7) {
            break 0;
        }
        @print_char(32);
    }
    @print_char(10);
    return;
}
